    <script type="text/javascript">
        $(document).ready(function () {
            checkSession();

            var layDSPhong = 'NhanVien/LayDanhSachPhong';
            var layKHTheoPhong = 'NhanVien/LayDanhSachTenKhachHangChungPhong/';
            var layChiPhiTheoPhong = 'NhanVien/LayThongTinChiPhiPhong/';
            loadDSPhong(layDSPhong);
            var maPhong;
            var cmnd;
            var nguoiDaiDien;
            $('body').on('click', '.info-box.bg-warning', function () {
                maPhong = $(this).attr('data-id');
                var info = {
                    idLoad: '#nguoidaidien-nhanp-ul',
                    url: layKHTheoPhong
                }
                LayThongTinPhong(info, maPhong);
                $('#nhanphongModal').modal('show');
            })

            $('body').on('click', '.info-box.bg-success', function () {
                maPhong = $(this).attr('data-id');
                var info = {
                    idCMND: '#cmnd-trap',
                    idNguoiDaiDien: '#nguoidaidien-trap',
                    idDichVu: '#dichvu-trap',
                    url: layChiPhiTheoPhong

                }
                LayThongTinPhong(info, maPhong);
                $('#traphongModal').modal('show');
            })
            $('body').on('click', '#btn-nhanphong', function () {
                cmnd = $('#cmnd-nhanp').val();
                nguoiDaiDien = $('#nguoidaidien-nhanp-ul').val();
                var dataInput = {
                    SoPhong: maPhong,
                    HoVaTen: nguoiDaiDien,
                    CMND: cmnd
                }
                var info = {
                    url: 'NhanVien/KhachHangNhanPhong',
                    modal: '#traphongModal',
                    urlLoad: layDSPhong
                }
                KhachHangNhanPhong(info, dataInput);
                //console.log(maPhong)
            })





        });

    </script>
